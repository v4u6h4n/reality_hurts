[Unit]
Description=v4l2loopback
After=network.target

[Service]
ExecStart=/usr/bin/sudo /usr/bin/modprobe v4l2loopback exclusive_caps=1 card_label='OBS Virtual Camera' video_nr=99; /usr/bin/sudo /usr/bin/modprobe v4l2loopback exclusive_caps=1 card_label='Virtual Logitech' video_nr=89

[Install]
WantedBy=default.target
